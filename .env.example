# SQLite database connection
DATABASE_URL="file:./prisma/dev.db"

# Authentication (single-user credentials)
# Replace with your email and a bcrypt hash generated locally.
AUTH_USER_EMAIL="me@example.com"
AUTH_USER_PASSWORD_HASH="$2a$12$replace_me_with_real_hash"

# NextAuth configuration
# NEXTAUTH_SECRET should be a long random string, e.g.:
# openssl rand -base64 32
NEXTAUTH_SECRET="change_me"
NEXTAUTH_URL="http://localhost:3000"
